{
  "name": "chrome-extensions-shopee",
  "description": "chrome extensions shopee",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "host_permissions": [
    "https://seller.shopee.cn/*"
  ],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://seller.shopee.cn/*"
      ]
    }
  ]
}
